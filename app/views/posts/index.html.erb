<% @category.posts.each do |p| %>
  <p>
    <strong>Title:</strong>
    <%= p.title %>
  </p>

  <p>
    <strong>Text:</strong>
    <%= p.description %>
  </p>
<% end %>

<%= link_to 'Edit Post', edit_category_post_path(@category) %> |
<%= link_to 'Back to List of Posts', category_path %> |
<%= link_to 'Back to Main Page', welcome_index_path %>